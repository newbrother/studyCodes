//http://codeup.kr/JudgeOnline/problem.php?id=1128
//1128 : n * 123456789 문제

#include <stdio.h>
int main(int argc, const char * argv[]) {
    int a[9]={1,2,3,4,5,6,7,8,9};
    int b[9]={0};
    int o10[10]={0};
    int o100[11]={0};
    int n;
    
    scanf("%d",&n);
    
    for(int i=8;i>=0;i--){
        if(a[i]*n%10+o10[i+1]+o100[i+2]>=10){
            b[i]=(a[i]*n%10+o10[i+1]+o100[i+2])%10;
            o10[i]= ((a[i]*n / 10) % 10)+1;
            o100[i]= a[i]*n / 100;
        }else{
            b[i]=a[i]*n%10+o10[i+1]+o100[i+2];
            o10[i]= (a[i]*n / 10) % 10;
            o100[i]= a[i]*n / 100;
        }
        if(o10[i]+o100[i+1]>=10){
            o10[i]%=10;
            o100[i]++;
        }
    }
    if(o10[0]+o100[1]>=10){
        o100[0]++;
    }
    if(o100[0]!=0){
    printf("%d",o100[0]);
    }
    if(o10[0]!=0 || o100[1]!=0){
    printf("%d",(o10[0]+o100[1])%10);
    }
    for(int i=0;i<9;i++){
        printf("%d",b[i]);
    }
    
    return 0;
}
