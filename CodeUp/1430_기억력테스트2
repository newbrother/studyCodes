//2중 반복을 사용하면 시간초과가 뜨는문제

#include <stdio.h>

int array[10000001]={};
int n,m,i,t;

int main(){
    scanf("%d",&n);
    for(i=0;i<n;i++){
        scanf("%d",&t);
        array[t]=1;
    }
    scanf("%d",&m);
    for(i=0;i<m;i++){
        scanf("%d",&t);
        printf("%d ",array[t]);
    }
}
