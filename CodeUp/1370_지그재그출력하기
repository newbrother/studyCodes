//http://codeup.kr/JudgeOnline/problem.php?id=1370
//1370 : 지그재그 출력하기

#include<stdio.h>
int main()
{
    int h,r; //높이:h , 반복횟수:r
    scanf("%d %d",&h,&r);
    
    for(int i=0;i<r;i++){
        for(int j=0;j<h;j++){
            for(int k=0;k<j;k++){
                printf(" ");
            }
            printf("*\n");
        }
        for(int j=h-1;j>0;j--){
            for(int k=j-1;k>0;k--){
                printf(" ");
            }
            printf("*\n");
        }
        
    }
    
    return 0;
}
